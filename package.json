{
  "name": "feathers-amqp-events-publish",
  "version": "0.3.1",
  "description": "Publish created, updated and removed events with using AMQP.",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "rm -rf ./dist && babel -d dist/ src/",
    "test": "ava -vs test/*.test.js && nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint --ignore-path .gitignore ./"
  },
  "dependencies": {
    "amqplib": "0.5.1",
    "babel-runtime": "6.23.0",
    "debug": "2.6.1",
    "lodash.map": "4.6.0"
  },
  "devDependencies": {
    "ava": "0.18.1",
    "babel-cli": "6.22.2",
    "babel-eslint": "6.0.4",
    "babel-plugin-transform-runtime": "6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-register": "6.22.0",
    "bluebird": "3.4.7",
    "body-parser": "1.16.0",
    "coveralls": "^2.13.1",
    "eslint": "2.10.2",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-import": "1.8.1",
    "feathers": "2.0.3",
    "feathers-hooks": "1.7.1",
    "feathers-mongoose": "4.0.1",
    "feathers-rest": "1.6.0",
    "mongoose": "4.8.1",
    "node-helpers": "1.1.8",
    "nyc": "^11.0.2",
    "shelljs": "0.7.8"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kozzztya/feathers-amqp-events.git"
  },
